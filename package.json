{
  "name": "@nut-tree/opencv-build",
  "version": "3.4.6-beta30",
  "description": "A simple script to auto build recent OpenCV + contrib version via npm. Base on [justadudewhohacks/npm-opencv-build](https://github.com/justadudewhohacks/npm-opencv-build)",
  "main": "./build/index.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "preinstall": "node ./preinstall.js",
    "preuninstall": "node ./preuninstall.js",
    "install": "node ./install.js",
    "test": "mocha --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nut-tree/npm-opencv-build.git"
  },
  "keywords": [
    "opencv",
    "build",
    "opencv4nodejs"
  ],
  "author": "s1hofmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nut-tree/npm-opencv-build/issues"
  },
  "homepage": "https://github.com/nut-tree/npm-opencv-build#readme",
  "dependencies": {
    "fs-extra": "^8.0.1",
    "npmlog": "^4.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "typescript": "^3.4.5"
  }
}